---
import Logo from "./Logo.astro";
import PageNavigation from "./PageNavigation.astro";
import SocialNetworks from "./SocialNetworks.astro";
import { PAGES } from "../constants/pages";
import { SOCIAL_NETWORKS } from "../constants/socialNetworks";
---

<div class="flex flex-col pt-24 pb-12 gap-6">
  <div class="flex flex-row justify-between items-center gap-4">
    <Logo />
    <SocialNetworks networks={SOCIAL_NETWORKS} />
  </div>
  <PageNavigation pages={PAGES} />
</div>
